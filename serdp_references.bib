@article{GaoGrassCanopyArchitecture2018,
  title = {Grass {{Canopy Architecture Influences Temperature Exposure}} at {{Soil Surface}}},
  volume = {1},
  copyright = {http://creativecommons.org/licenses/by/3.0/},
  doi = {10.3390/fire1030035},
  abstract = {There is increasing recognition that plant traits contribute to variations in fire behavior and fire regime. Diversity across species in litter flammability and canopy flammability has been documented in many woody plants. Grasses, however, are often considered homogeneous fuels in which any flammability differences across species are attributable to biomass differences alone and therefore are of less ecological interest, because biomass is hugely plastic. We examined the effect of grass canopy architecture on flammability across eight grass species in short grass steppe of New Mexico and Texas. To characterize grass canopy architecture, we measured biomass density and \&ldquo;biomass-height ratio\&rdquo; (the ratio of canopy biomass above 10 cm to that of biomass below 10 cm). Indoor flammability experiments were performed on air-dried individual plants. As expected, plant biomass influenced all flammability measures. However, biomass-height ratio had additional negative effect on temperature exposure at soil surface (accumulation of mean temperature \&gt;100 \&deg;C) in well-cured grasses, which is an important fire behavior metric predicting soil heating and meristem survival. This canopy architecture effect, however, needs further investigation to be isolated from biomass density due to correlation of these two traits. This result demonstrates the potential for species-specific variation in architecture to influence local fire effects in grasses.},
  language = {en},
  number = {3},
  journal = {Fire},
  author = {Gao, Xiulin and Schwilk, Dylan W.},
  month = dec,
  year = {2018},
  keywords = {fire,grasslands,canopy architecture,grass fire ecological impact,grass flammability,meristem survival,soil heating},
  pages = {35},
  file = {/Users/whalendillon/Zotero/storage/U5DV8UQW/Gao_Schwilk_2018_Grass Canopy Architecture Influences Temperature Exposure at Soil Surface.pdf;/Users/whalendillon/Zotero/storage/8YUFLKK7/htm.html}
