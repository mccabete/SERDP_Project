@article{Kampichler2013,
  title = {Determining Patterns of Variability in Ecological Communities: {{Time}} Lag Analysis Revisited},
  volume = {20},
  issn = {13528505},
  doi = {10.1007/s10651-012-0219-y},
  abstract = {All ecological communities experience change over time. One method to$\backslash$nquantify temporal variation in the patterns of relative abundance of$\backslash$ncommunities is time lag analysis (TLA). It uses a distance-based$\backslash$napproach to study temporal community dynamics by regressing community$\backslash$ndissimilarity over increasing time lags (one-unit lags, two-unit lags,$\backslash$nthree-unit lags). Here, we suggest some modifications to the method and$\backslash$nrevaluate its potential for detecting patterns of community change. We$\backslash$napply Hellinger distance based TLA to artificial data simulating$\backslash$ncommunities with different levels of directional and stochastic dynamics$\backslash$nand analyse their effects on the slope and its statistical significance.$\backslash$nWe conclude that statistical significance of the TLA slope (obtained by$\backslash$na Monte Carlo permutation procedure) is a valid criterion to$\backslash$ndiscriminate between (i) communities with directional change in species$\backslash$ncomposition, regardless whether it is caused by directional abundance$\backslash$nchange of the species or by stochastic change according to a Markov$\backslash$nprocess, and (ii) communities that are composed of species with$\backslash$npopulation sizes oscillating around a constant mean or communities whose$\backslash$nspecies abundances are governed by a white noise process. TLA slopes$\backslash$nrange between 0.02 and 0.25, depending on the proportions of species$\backslash$nwith different dynamics; higher proportions of species with constant$\backslash$nmeans imply shallower slopes; and higher proportions of species with$\backslash$nstochastic dynamics or directional change imply steeper slopes. These$\backslash$nvalues are broadly in line with TLA slopes from real world data. Caution$\backslash$nmust be exercised when TLA is used for the comparison of community time$\backslash$nseries with different lengths since the slope depends on time series$\backslash$nlength and tends to decrease non-linearly with it.},
  number = {2},
  journal = {Environmental and Ecological Statistics},
  author = {Kampichler, Christian and {van der Jeugd}, Henk P.},
  year = {2013},
  keywords = {Community change,Markov process,Species composition,Stochasticity,Temporal dynamics},
  pages = {271-284}
}

