  pages = {389-396},
  isbn = {0044-7447},
  mendeley-tags = {CHANS,biodiversity,chans,conservation,disturbance,ecological communities,landscape ecology,resilience},
  pmid = {14627367}
}

@article{Kearney2014,
  title = {Microclimate Modelling at Macro Scales: {{A}} Test of a General Microclimate Model Integrated with Gridded Continental-Scale Soil and Weather Data},
  volume = {5},
  issn = {2041210X},
  doi = {10.1111/2041-210X.12148},
  abstract = {The microclimate experienced by organisms is determined by local weather conditions. Yet the environmental data available for predicting the effect of climate on the distribution and abundance of organisms are typically in the form of long-term average monthly climate measured at standardized heights above the ground. Here, we demonstrate how hourly microclimates can be modelled mechanistically over decades at the continental scale with biologically suitable accuracy. We extend the microclimate model of the software package niche mapper to capture spatial and temporal variation in soil thermal properties and integrate it with gridded soil and weather data for Australia at 0\textperiodcentered{}05$^\circ$ resolution. When tested against historical observations of soil temperature, the microclimate model predicted 85\% of the variation in hourly soil temperature across 10 years from the surface to 1 m deep with an accuracy of 2\textendash{}3\textperiodcentered{}3 $^\circ$C (c. 10\% of the temperature range at a given depth) across an extremely climatically diverse range of sites. This capacity to accurately and mechanistically predict hourly local microclimates across continental scales creates new opportunities for understanding how organisms respond to changes in climate.},
  journal = {Methods in Ecology and Evolution},
  author = {Kearney, Michael R. and Shamakhy, Alireza and Tingley, Reid and Karoly, David J. and a. Hoffmann, Ary and Briggs, Peter R. and Porter, Warren P.},
  year = {2014},
  keywords = {climate,microclimate,Microclimate,modeling,Weather,environmental modeling,Climate,macroecology,Biophysical ecology,Mechanistic model,Soil temperature},
  pages = {273-286},
  isbn = {2041210X},
