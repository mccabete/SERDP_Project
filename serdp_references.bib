  keywords = {fire,fire ecology,disturbance,correspondence,land use,appalachian mountains,charles lafon,department,fire geography,fire history,fire interval,land change,spatial patterns,temperate forest,tree ring,vegetation dynamics},
  pages = {2093-2104},
