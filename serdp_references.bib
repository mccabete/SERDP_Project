  journal = {Ecology},
  author = {Chapman, Teresa B and Veblen, Thomas T and Schoennagel, Tania},
  month = oct,
  year = {2012},
  keywords = {fire,usa,bark beetle,contorta,dendroctonus ponderosae,epicenter,epidemic,lodgepole pine,pinus,pinus ponderosa,ponderosa pine,rocky mountains,spatial synchrony},
  pages = {2175-85},
