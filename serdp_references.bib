  keywords = {forest ecology,disturbance,competition,climate change,dna,evolutionary,forest trees,gap characteristics,gene flow,habitat fragmentation,local adaptation,microsatellites,migration rates,plant migration,responses,Science {\\\&} Technology - Other Topics,seed dispersal},
