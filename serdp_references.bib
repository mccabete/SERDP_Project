@article{Rangan2009,
  title = {What Makes Ecology ‘political'?: Rethinking ‘scale' in Political Ecology},
  volume = {33},
  doi = {10.1177/0309132508090215},
  abstract = {This essay explores the ways in which concepts of ‘scale' are deployed in political ecology to explain the outcomes of ecological and social change. It argues that political ecologists need to pay closer attention to how scale is produced and used to interpret the experience of spatiotemporal difference and change so as to make ecology the object of politics, policy-making and political action. It outlines an alternative approach that focuses on how three moments of action \textemdash{} operation, observation, and interpretation \textemdash{} work together to produce scale as a configuration and range of values that articulate differential sensibilities and political differences regarding changes to socialized landscapes. The essay uses examples from studies of plant movements to illustrate how scope and scale combine to ‘enframe' and interpret ecological and related social change as ‘disruption' to places, regional ‘transformation', or as regionalized ‘evolution'.},
  number = {1},
  journal = {Progress in Human Geography},
