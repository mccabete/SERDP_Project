  author = {Austin, Mike},
  month = jan,
  year = {2007},
  keywords = {Competition,ecological modeling,sdm,species distributions,environmental modeling,sem,glm,Environmental gradients,Generalized additive model,Generalized linear model,Geographically weighted regression,Quantile regression,Species response curves,Structural equation modelling},
