  author = {Dell, Jane E. and Richards, Lora A. and O'Brien, Joseph J. and Loudermilk, E. Louise and Hudak, Andrew T. and Pokswinski, Scott M. and Bright, Benjamin C. and Hiers, J. Kevin and Williams, Brett W. and Dyer, Lee A.},
