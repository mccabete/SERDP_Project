  publisher = {{U.S. Department of Agriculture, Forest Service, Rocky Mountain Research Station}},
  author = {Prisley, Stephen P SP and Wang, HJ Huei-jin and Radtke, Philip J PJ and Coulston, John},
  editor = {McWilliams, Will and Moisen, Gretchen and Czaplewski, Ray},
  month = oct,
  year = {2009},
  keywords = {fire,disturbance,remote sensing,productivity,climate change,elevation,aspect,environmental monitoring,terrain,assessments,environmental m,national forest inventory (NFI),site quality,slope},
  pages = {1-10}
