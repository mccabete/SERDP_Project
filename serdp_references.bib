  year = {2010},
  keywords = {disease ecology,invasion ecology,plant disease,disease-environment,Biological invasions,dissertation,qual exam,plant pathology,Biotic resistance,Clovers,Escape from natural enemies,Host range of plant pathogenic fungi,Medicago,Rapid evolution,Trifolium},
