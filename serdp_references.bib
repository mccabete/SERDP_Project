  pages = {e01964},
  file = {/Users/whalendillon/Zotero/storage/68WF2ZYB/Dell et al_2017_Overstory-derived surface fuels mediate plant species diversity in frequently.pdf;/Users/whalendillon/Zotero/storage/BKRDTEHQ/Dell et al_2017_Overstory-derived surface fuels mediate plant species diversity in frequently.pdf;/Users/whalendillon/Zotero/storage/Z8M7CGFV/ecs2.html},
