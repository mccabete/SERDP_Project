  keywords = {Habitat fragmentation,Landscape epidemiology,Crop resistance,Functional diversity,Spatial heterogeneity,Stochastic modelling},
  pages = {35-47},
  file = {/Users/whalendillon/Zotero/storage/GX35KSTM/Papaïx et al_2014_Can epidemic control be achieved by altering landscape connectivity in.pdf}
}

