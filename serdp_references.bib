@article{GilbertAltitudinalpatternstick2010,
  title = {Altitudinal Patterns of Tick and Host Abundance: A Potential Role for Climate Change in Regulating Tick-Borne Diseases?},
  volume = {162},
  issn = {0029-8549, 1432-1939},
  shorttitle = {Altitudinal Patterns of Tick and Host Abundance},
  doi = {10.1007/s00442-009-1430-x},
  abstract = {The impact of climate change on vector-borne infectious diseases is currently controversial. In Europe the primary arthropod vectors of zoonotic diseases are ticks, which transmit Borrelia burgdorferi sensu lato (the agent of Lyme disease), tick-borne encephalitis virus and louping ill virus between humans, livestock and wildlife. Ixodes ricinus ticks and reported tick-borne disease cases are currently increasing in the UK. Theories for this include climate change and increasing host abundance. This study aimed to test how I. ricinus tick abundance might be inXuenced by climate change in Scotland by using altitudinal gradients as a proxy, while also taking into account the eVects of hosts, vegetation and weather eVects. It was predicted that tick abundance would be higher at lower altitudes (i.e. warmer climates) and increase with host abundance. Surveys were conducted on nine hills in Scotland, all of open moorland habitat. Tick abundance was positively associated with deer abundance, but even after taking this into account, there was a strong negative association of ticks with altitude. This was probably a real climate eVect, with temperature (and humidity, i.e. saturation deWcit) most likely playing an important role. It could be inferred that ticks may become more abundant at higher altitudes in response to climate warming. This has potential implications for pathogen prevalence such as louping ill virus if tick numbers increase at elevations where competent transmission hosts (red grouse Lagopus lagopus scoticus and mountain hares Lepus timidus) occur in higher numbers.},
  language = {en},
  number = {1},
  journal = {Oecologia},
  author = {Gilbert, Lucy},
  month = jan,
  year = {2010},
  pages = {217-225},
  file = {/Users/whalendillon/Zotero/storage/59JWZWWE/Gilbert - 2010 - Altitudinal patterns of tick and host abundance a.pdf}
}

@article{MorrisChoosingusingdiversity2014a,
  title = {Choosing and Using Diversity Indices: Insights for Ecological Applications from the {{German Biodiversity Exploratories}}},
  volume = {4},
  issn = {20457758},
  shorttitle = {Choosing and Using Diversity Indices},
  doi = {10.1002/ece3.1155},
  abstract = {Biodiversity, a multidimensional property of natural systems, is difficult to quantify partly because of the multitude of indices proposed for this purpose. Indices aim to describe general properties of communities that allow us to compare different regions, taxa, and trophic levels. Therefore, they are of fundamental importance for environmental monitoring and conservation, although there is no consensus about which indices are more appropriate and informative. We tested several common diversity indices in a range of simple to complex statistical analyses in order to determine whether some were better suited for certain analyses than others. We used data collected around the focal plant Plantago lanceolata on 60 temperate grassland plots embedded in an agricultural landscape to explore relationships between the common diversity indices of species richness (S), Shannon's diversity (H'), Simpson's diversity (D1), Simpson's dominance (D2), Simpson's evenness (E), and Berger\textendash{}Parker dominance (BP). We calculated each of these indices for herbaceous plants, arbuscular mycorrhizal fungi, aboveground arthropods, belowground insect larvae, and P. lanceolata molecular and chemical diversity. Including these trait-based measures of diversity allowed us to test whether or not they behaved similarly to the better studied species diversity. We used path analysis to determine whether compound indices detected more relationships between diversities of different organisms and traits than more basic indices. In the path models, more paths were significant when using H', even though all models except that with E were equally reliable. This demonstrates that while common diversity indices may appear interchangeable in simple analyses, when considering complex interactions, the choice of index can profoundly alter the interpretation of results. Data mining in order to identify the index producing the most significant results should be avoided, but simultaneously considering analyses using multiple indices can provide greater insight into the interactions in a system.},
  language = {en},
  number = {18},
  journal = {Ecology and Evolution},
  author = {Morris, E. Kathryn and Caruso, Tancredi and Buscot, Fran{\c c}ois and Fischer, Markus and Hancock, Christine and Maier, Tanja S. and Meiners, Torsten and M\"uller, Caroline and Obermaier, Elisabeth and Prati, Daniel and Socher, Stephanie A. and Sonnemann, Ilja and W\"aschke, Nicole and Wubet, Tesfaye and Wurst, Susanne and Rillig, Matthias C.},
  month = sep,
  year = {2014},
  pages = {3514-3524},
  file = {/Users/whalendillon/Zotero/storage/9D2CW8MQ/ece30004-3514-sd8.csv;/Users/whalendillon/Zotero/storage/AFDQ7XAM/ece30004-3514-sd2.csv;/Users/whalendillon/Zotero/storage/GLNPBB6F/Morris et al. - 2014 - Choosing and using diversity indices insights for.pdf;/Users/whalendillon/Zotero/storage/K3Z4RJYJ/ece30004-3514-sd7.csv;/Users/whalendillon/Zotero/storage/LBK8ZWWQ/ece30004-3514-sd6.csv;/Users/whalendillon/Zotero/storage/QJUSSW4S/ece30004-3514-sd3.csv;/Users/whalendillon/Zotero/storage/SV8GAEGM/ece30004-3514-sd9.doc;/Users/whalendillon/Zotero/storage/SZ258FAI/ece30004-3514-sd1.R;/Users/whalendillon/Zotero/storage/XCUW98R3/ece30004-3514-sd4.csv;/Users/whalendillon/Zotero/storage/YXP9LBGD/ece30004-3514-sd5.csv;/Users/whalendillon/Zotero/storage/Z5DRGSWK/Morris-etal-2014-Choosing-using-diversity-indices.pdf}
