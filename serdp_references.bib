@article{Spasojevic2016,
  title = {Scaling up the Diversity-Resilience Relationship with Trait??Databases and Remote Sensing Data: {{The}} Recovery of??Productivity after Wildfire},
  volume = {22},
  issn = {13652486},
  doi = {10.1111/gcb.13174},
  abstract = {Understanding the mechanisms underlying ecosystem resilience - why some systems have an irreversible response to disturbances while others recover - is critical for conserving biodiversity and ecosystem function in the face of global change. Despite the widespread acceptance of a positive relationship between biodiversity and resilience, empirical evidence for this relationship remains fairly limited in scope and localized in scale. Assessing resilience at the large landscape and regional scales most relevant to land management and conservation practices has been limited by the ability to measure both diversity and resilience over large spatial scales. Here, we combined tools used in large-scale studies of biodiversity (remote sensing and trait databases) with theoretical advances developed from small-scale experiments to ask whether the functional diversity within a range of woodland and forest ecosystems influences the recovery of productivity after wildfires across the four-corner region of the United States. We additionally asked how environmental variation (topography, macroclimate) across this geographic region influences such resilience, either directly or indirectly via changes in functional diversity. Using path analysis, we found that functional diversity in regeneration traits (fire tolerance, fire resistance, resprout ability) was a stronger predictor of the recovery of productivity after wildfire than the functional diversity of seed mass or species richness. Moreover, slope, elevation, and aspect either directly or indirectly influenced the recovery of productivity, likely via their effect on microclimate, while macroclimate had no direct or indirect effects. Our study provides some of the first direct empirical evidence for functional diversity increasing resilience at large spatial scales. Our approach highlights the power of combining theory based on local-scale studies with tools used in studies at large spatial scales and trait databases to understand pressing environmental issues.},
  number = {4},
  journal = {Global Change Biology},
  author = {Spasojevic, Marko J. and Bahlai, Christie A. and Bradley, Bethany A. and Butterfield, Bradley J. and Tuanmu, Mao Ning and Sistla, Seeta and Wiederholt, Ruscena and Suding, Katharine N.},
  year = {2016},
  keywords = {Path analysis,Southwest United States,Functional diversity,Fire resistance,Fire tolerance,Landfire,MODIS,NatureServe,Resprout ability,Seed mass},
  pages = {1421-1432},
  isbn = {1354-1013},
  pmid = {26599833}
}

@article{Herben2016,
  title = {A Quest for Species-Level Indicator Values for Disturbance},
  volume = {27},
  issn = {16541103},
  doi = {10.1111/jvs.12384},
  abstract = {$\backslash$nQuestion$\backslash$nIs it possible to construct indicator values that would place individual species on gradients of disturbance frequency and severity? We suggest that such indicator values could be defined on the basis of the disturbance regimes under which individual species occur, to be independent of their traits. They should also separate disturbance frequency from disturbance severity.$\backslash$n$\backslash$nLocation$\backslash$nCzech Republic.$\backslash$n$\backslash$nMethods$\backslash$nWe used a stratified set of 30 115 vegetation-plot records sampled over the whole country and classified into 39 phytosociological vegetation classes. Each class was assigned values of disturbance frequency and severity by expert judgement. A Disturbance Frequency Index for each species was calculated as the mean of the common logarithm of the disturbance frequency of all vegetation classes weighted by occurrence frequencies of this species in those classes. A Disturbance Severity Index was defined as mean disturbance severity of all vegetation classes weighted by occurrence frequencies of this species in those classes. For forest vegetation, indices were computed separately for the whole community and for the herb layer, which experiences different types of disturbance. Further, we constructed a disturbance index from vegetation structural parameters, viz. summed covers and community-weighted mean of height at maturity of species recorded in each plot for each vegetation class. We assessed all indices by comparing their values with data on functional traits of the species.$\backslash$n$\backslash$nResults$\backslash$nWe calculated values of the indices for 1248 species. The Disturbance Frequency Index and Disturbance Severity Index were correlated, but still described different responses of individual species to disturbance. The index based on vegetation structure was correlated mainly with the Disturbance Frequency Index. All indices showed strong relationships to plant traits: species with high values of all disturbance indices tended to have small seeds, to be annual and non-clonal or able to spread clonally over large distances.$\backslash$n$\backslash$nConclusions$\backslash$nConstructing species disturbance indices based on vegetation characteristics, not plant traits, is feasible and provides meaningful scores. A similar approach can be used in any region where sufficient vegetation data are available. Disturbance indices can be used to address a number of questions in plant evolution and community or landscape ecology.$\backslash$n},
  number = {3},
  journal = {Journal of Vegetation Science},
  author = {Herben, Tom???? and Chytr??, Milan and Klime??ov??, Jitka},
  year = {2016},
  keywords = {Vegetation structure,Bioindication,Clonal traits,Disturbance frequency,Disturbance intensity,Disturbance severity,Ecological indicators,Expert judgement,Plant height at maturity,Plant longevity,Seed size,Vascular plants},
  pages = {628-636}
}

@article{Peet1999,
  title = {The Conservation Management of {{Imperata}} Cylindrica Grassland in {{Nepal}} with Fire and Cutting: {{An}} Experimental Approach},
  volume = {36},
  issn = {00218901},
  doi = {10.1046/j.1365-2664.1999.00405.x},
  abstract = {1. Imperata cylindrica dominated grassland in the Nepal Teral supports a$\backslash$nnumber of threatened species including swamp deer Cervus duvauceli,$\backslash$nhispid hare Caprolagus hispidus and Bengal florican Eupodotis$\backslash$nbengalensis. It is also the main source of thatch material for local$\backslash$ncommunities. Current widespread cutting and burning of the grasslands is$\backslash$ndeleterious to cover-dependent vertebrates which would benefit from$\backslash$npatches of grassland remaining unmanaged.$\backslash$n2. In Royal Bardia National Park, a randomized block experiment with$\backslash$nfour treatments (cutting, burning, cutting and burning, and no$\backslash$nmanagement) was established to examine the effects of these treatments$\backslash$non plant species abundance, species richness and grassland structure.$\backslash$nThe grassland was dominated by Imperata cylindrica, with Saccharum$\backslash$nspontaneum, Vetiveria zizanioides, Desmostachya bipinnata and$\backslash$nSchizachytrium brevifolium the other abundant species.$\backslash$n3. In year 3, 2 years after the first treatment, Imperata cylindrica$\backslash$nremained the dominant grass species under all treatments. It increased$\backslash$nin abundance in unmanaged plots and declined in abundance in managed$\backslash$nplots. Desmostachya bipinnata showed the opposite response. Plant$\backslash$nspecies richness was significantly higher in managed plots, which were$\backslash$nstructurally more heterogeneous.$\backslash$n4. The effects of cutting alone, burning alone, and cutting and burning$\backslash$ncombined on the structure and composition of the grassland were similar,$\backslash$ndespite additive effects on total grass cover, total forb cover and forb$\backslash$nspecies richness in cut and burned plots. This allows the effects of$\backslash$ncutting and burning to be reduced to a single treatment (cutting and$\backslash$nburning) in future experiments to investigate the impacts of patch$\backslash$nharvesting on community dynamics. Simpler factorial experiments can be$\backslash$ndesigned than if cutting and burning had to be considered as separate$\backslash$ntreatments.$\backslash$n5. There was no difference in the establishment of tall grass species or$\backslash$nwoody species between managed and unmanaged plots.$\backslash$n6. Results suggest that patches of grassland could be left unmanaged on$\backslash$na 2-year rotation without significantly altering the composition of the$\backslash$nplant community, thereby providing refugia for cover-dependent faunal$\backslash$nspecies.},
  number = {3},
  journal = {Journal of Applied Ecology},
  author = {Peet, Nicholas B. and Watkinson, Andrew R. and Bell, Diana J. and Sharma, Uday R.},
  year = {1999},
  keywords = {Burn,Patch management,Protected area,Terai},
  pages = {374-387},
