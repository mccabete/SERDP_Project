@article{VanHornLandscapePhysiognomyInfluences2018,
  title = {Landscape {{Physiognomy Influences Abundance}} of the {{Lone Star Tick}}, {{Amblyomma}} Americanum ({{Ixodida}}: {{Ixodidae}}), in {{Ozark Forests}}},
