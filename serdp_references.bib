  author = {Holmes, Thomas P. and Aukema, Juliann E. and Von Holle, Betsy and Liebhold, Andrew and Sills, Erin},
  year = {2009},
  keywords = {forest ecology,invasive species,invasion ecology,Pathogens,Nonnative,Ecosystem services,Chestnut blight,Contingent valuation,Economic,economics,Forest pests,Gypsy moth,Hedonics,Invasive,Management,Trade},
  pages = {18-38},
  file = {/Users/whalendillon/Zotero/storage/GKG79GKG/Holmes et al_2009_Economic impacts of invasive species in forests.pdf},
  isbn = {9781573317535},
  mendeley-tags = {economics,forest ecology,invasion ecology,invasive species},
