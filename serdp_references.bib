  month = jun,
  year = {2012},
  keywords = {fire,Ecosystem,Biological,Models,Trees,Forestry,Environmental Monitoring,Conservation of Natural Resources: economics,Conservation of Natural Resources: methods,Forestry: economics,Human Activities,Quebec},
