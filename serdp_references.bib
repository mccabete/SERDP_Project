  year = {2006},
  keywords = {fire,Fire intensity,longleaf pine,Pinus palustris,Longleaf pine,Environmental heterogeneity,Fire temperature,Fuel consumption,Fuel load,Ground cover,Louisiana (USA),Southeastern United States,Vegetation gaps},
  pages = {1331-1337},
  file = {/Users/whalendillon/Zotero/storage/DN336QAB/Thaxton_Platt_2006_Small-scale fuel variation alters fire intensity and shrub abundance in a pine.pdf},
