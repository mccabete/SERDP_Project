@article{Aguilar-TipacamuEffectmoxidectinnatural2003,
  title = {Effect of Moxidectin against Natural Infestation of the Cattle Tick {{Boophilus}} Microplus ({{Acarina}}: {{Ixodidae}}) in the {{Mexican}} Tropics},
  volume = {111},
  issn = {0304-4017},
  shorttitle = {Effect of Moxidectin against Natural Infestation of the Cattle Tick {{Boophilus}} Microplus ({{Acarina}}},
  doi = {10.1016/S0304-4017(02)00355-2},
  abstract = {The study was divided in to two trials and carried out in a ranch in eastern Yucatan state, Mexico. In the first trial, two groups of 15 Bostaurus\texttimes{}Bosindicus heifers, 6\textendash{}12 month of age and naturally infested with Boophilus microplus ticks were used. Heifers in Group 1 were treated with a 1\% injectable formulation of moxidectin at the dose of 0.20mg/kg body weight by subcutaneous injection. The other group remained as untreated controls. Number of immature and engorging female ticks were assessed on days 0, 7, 14, 21, 28 and 35 post-treatment (PT). The efficacy of moxidectin on adult ticks from day 7 to 28 PT was greater than 95\%. The efficacy decreased to 74.9\% by day 35. In the second trial, animals in Group 1 were treated with the moxidectin product as before, while cattle in Group 2 were treated according to the routine procedure for the control of ticks on that property (125g/l amitraz as a dip). Treatment of all cattle was repeated four times at intervals of 28 days. The efficacy of the experimental moxidectin treatment was similar to that of the routine amitraz treatment, i.e., greater than 99\%.},
  number = {2},
  journal = {Veterinary Parasitology},
  author = {{Aguilar-Tipacamu}, G and {Rodriguez-Vivas}, R. I},
  month = feb,
  year = {2003},
  keywords = {Mexico,Cattle,Amitraz,Control,Moxidectin},
  pages = {211-216},
  file = {/Users/whalendillon/Zotero/storage/VPBZ3ZK4/Aguilar-Tipacamu_Rodriguez-Vivas_2003_Effect of moxidectin against natural infestation of the cattle tick Boophilus.pdf;/Users/whalendillon/Zotero/storage/62E9FJLE/S0304401702003552.html}
}

