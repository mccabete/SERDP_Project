  pages = {S79-S89},
  file = {/Users/whalendillon/Zotero/storage/P69IRE9M/Power_Mitchell_2004_Pathogen spillover in disease epidemics.pdf},
  mendeley-tags = {apparent competition,pathogen spillover},
