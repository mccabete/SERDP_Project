  year = {2014},
  keywords = {fire,Fire effects,Fire exclusion,Resprouting,Tree mortality,Alternative vegetation states,Conifer encroachment,Quercus kelloggii,Top-kill},
  pages = {94-107},
  file = {/Users/whalendillon/Zotero/storage/UZEAFFRQ/Cocking et al_2014_Long-term effects of fire severity on oak-conifer dynamics in the southern.pdf},
  isbn = {1051-0761},
