  keywords = {field ecology,Field ecology,infectious diseases,Infectious diseases,mathematical models,Mathematical models,statistical models,Statistical models,study design,Study design,wildlife,wildlife epidemiology,Wildlife epidemiology},
  pages = {1083-1094},
