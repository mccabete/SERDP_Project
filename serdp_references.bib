  keywords = {Biodiversity,Ecosystem,Biomass,Carbon,Carbon: metabolism,Species Specificity,Trees,Trees: anatomy {\\\&} histology,Trees: growth {\\\&} development,Tropical Climate,Rain,Population Dynamics,Brazil,Carbon Dioxide,Carbon Dioxide: metabolism,Disasters,Environment,Forestry},
  pages = {2259-69},
  file = {/Users/whalendillon/Zotero/storage/QLHJHCSS/Nepstad et al_2007_Mortality of large trees and lianas following experimental drought in an Amazon.pdf},
