@article{OBrienCanopyDerivedFuelsDrive2016,
  title = {Canopy-{{Derived Fuels Drive Patterns}} of {{In}}-{{Fire Energy Release}} and {{Understory Plant Mortality}} in a {{Longleaf Pine}} ( {{{\emph{Pinus}}}}{\emph{ Palustris}} ) {{Sandhill}} in {{Northwest Florida}}, {{USA}}},
