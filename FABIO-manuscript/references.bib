@article{EtlingerDevelopmentlaboratoryprotocol2004,
  title = {Development of a Laboratory Protocol for Fire Performance of Landscape Plants},
  volume = {13},
  issn = {1049-8001},
  doi = {10.1071/WF04039},
  abstract = {Over 2.5 million people and 1 million structures risk destruction from wildland fires in California. One way to mitigate this risk is the manipulation and/or selection of landscape vegetation. In this study, six species were studied for their intrinsic characteristics and tested with a newly-developed laboratory fire protocol at 150 kW to determine heat release rate (HRR). The plants were 2\textendash{}3 years old and obtained from a local nursery. Whole plants were subjected to desiccation in a dry kiln at 50$\smwhtcircle$C, which was found to be much more effective than simulating fire weather in a greenhouse. This is apparently the first study that has measured plant variables, burned them in a natural vertical position, and related HRR to the plant characteristics. Multiple regression showed the overwhelming importance of foliage and moisture content to peak HRR. Chemical variables were not significant and high moisture contents were found to obscure other plant characteristics.},
  language = {en},
